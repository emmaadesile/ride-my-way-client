{
  "presets": [
    "react",
    [
      "env",
      {
        "targets": {
          "browsers": ["last 2 versions"]
        }
      }
    ]
  ],
  "plugins": [
    "transform-class-properties"
  ]
}

{
  "presets":  [["es2015", { "modules": false }], "react"],
  "env": {
  "test": {
      "presets":[
        "es2015",
         "stage-0",
        "react"],
      "plugins": [
        "transform-es2015-modules-commonjs",
        "dynamic-import-node"
      ]
    }
  }
}